msgid "Repositories enabled with enman:"
msgstr "Repositórios ativados pelo enman:"

#: lib/App/enman/Command/remove.pm:29
#, perl-brace-format
msgid "removing '{repo_name}' - '{repository}'"
msgstr "removendo '{repo_name}' - '{repository}'"

